{
  "name": "GCP Batch Task Example",
  "inputs": [
    {
      "url": "gs://tes-batch-integration/inputs/README.md",
      "path": "/mnt/disks/tes-batch-integration/inputsREADME.md"
    }
  ],
  "outputs": [
    {
      "url": "gs://tes-batch-integration/outputs/README.md.sha256",
      "path": "/mnt/disks/tes-batch-integration/outputs/README.md.sha256"
    }
  ],
  "executors": [
    {
      "image": "alpine",
      "command": [
        "sha256sum",
        "/mnt/disks/tes-batch-integration/inputs/README.md | tee /mnt/disks/tes-batch-integration/outputs/README.md.sha256"
      ]
    }
  ]
}
