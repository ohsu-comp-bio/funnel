{
    "configurations": [
    {
        "name": "Attach to Process",
        "type": "go",
        "request": "attach",
        "mode": "local",
        "processId": "${command:pickProcess}",
    },
    {
        "name": "Launch file",
        "type": "go",
        "request": "launch",
        "mode": "auto",
        "program": "${file}",
        "args": ["${workspaceFolder}/tes/task-execution-schemas/openapi/task_execution_service.openapi.yaml"],
    }
    ]
}